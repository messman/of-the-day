{
	"name": "oftheday-client",
	"version": "2.0.0",
	"description": "Mobile-first web application to show of-the-day information.",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config webpack.dev.js",
		"dev": "webpack --config webpack.dev.js",
		"dev:w": "webpack --config webpack.dev.js --watch",
		"dev:s": "webpack-dev-server --config webpack.dev.js",
		"static": "cd dist && python -m SimpleHTTPServer 8888",
		"production": "webpack --config webpack.prod.js",
		"production:w": "webpack --config webpack.prod.js --watch",
		"analyze-bundle": "webpack-bundle-analyzer --port 4200 dist/stats.json",
		"storybook": "start-storybook --port=58888"
	},
	"author": "Andrew Messier",
	"license": "MIT",
	"dependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-is": "^16.13.1",
		"styled-components": "^5.1.1",
		"oftheday-shared": "file:../shared"
	},
	"devDependencies": {
		"@babel/core": "^7.10.3",
		"@babel/preset-env": "^7.10.3",
		"@babel/preset-react": "^7.10.1",
		"@storybook/addon-knobs": "^5.3.19",
		"@storybook/addon-viewport": "^5.3.19",
		"@storybook/react": "^5.3.19",
		"@svgr/webpack": "^5.4.0",
		"@types/node": "^14.0.14",
		"@types/react": "^16.9.41",
		"@types/react-dom": "^16.9.8",
		"@types/styled-components": "^5.1.0",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.0.2",
		"html-webpack-plugin": "^4.3.0",
		"regenerator-runtime": "^0.13.5",
		"typescript": "^3.9.5",
		"typescript-plugin-styled-components": "^1.4.4",
		"webpack": "^4.43.0",
		"webpack-bundle-analyzer": "^3.8.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.2"
	}
}
