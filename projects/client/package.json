{
	"name": "oftheday-client",
	"version": "2.0.2",
	"description": "Mobile-first web application to show of-the-day information.",
	"main": "index.js",
	"scripts": {
		"link": "npm link oftheday-shared",
		"build": "webpack --config webpack.dev.js",
		"dev": "webpack --config webpack.dev.js",
		"dev:w": "webpack --config webpack.dev.js --watch",
		"dev:s": "webpack-dev-server --config webpack.dev.js",
		"static": "cd dist && python -m SimpleHTTPServer 8888",
		"production": "webpack --config webpack.prod.js",
		"production:w": "webpack --config webpack.prod.js --watch",
		"production:analyze": "PROD_ANALYZE=true webpack --config webpack.prod.js",
		"analyze-bundle": "webpack-bundle-analyzer --port 4200 dist/stats.json",
		"storybook": "start-storybook --port=58888"
	},
	"author": "Andrew Messier",
	"license": "MIT",
	"dependencies": {
		"@messman/react-common": "^1.4.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-is": "^16.13.1",
		"react-router-dom": "^5.2.0",
		"react-spring": "^8.0.27",
		"seamless-scroll-polyfill": "^1.2.3",
		"styled-components": "^5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-react": "^7.10.4",
		"@storybook/addon-knobs": "^5.3.21",
		"@storybook/addon-viewport": "^5.3.21",
		"@storybook/react": "^5.3.21",
		"@svgr/webpack": "^5.4.0",
		"@types/node": "^14.11.8",
		"@types/react": "^16.9.51",
		"@types/react-dom": "^16.9.8",
		"@types/react-router-dom": "^5.1.6",
		"@types/styled-components": "^5.1.4",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.2.1",
		"html-webpack-plugin": "^4.5.0",
		"regenerator-runtime": "^0.13.7",
		"ts-loader": "^8.0.4",
		"typescript": "^3.9.7",
		"typescript-plugin-styled-components": "^1.4.4",
		"url-loader": "^4.1.1",
		"webpack": "^4.44.2",
		"webpack-bundle-analyzer": "^3.9.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.2.0"
	}
}
